{
	"workbench.iconTheme": "vscode-icons",
	"window.zoomLevel": 0,
	"prettier.printWidth": 120,
	"prettier.singleQuote": true,
	"editor.hover.enabled": true,
	"editor.wordWrap": "on",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"editor.quickSuggestionsDelay": 0,
	"editor.tabSize": 2,
	"editor.formatOnSave": true,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"ejs": "html"
	},
	"html.format.unformatted": "wbr,%",
	"files.associations": {
		"*.ejs": "html"
	},
	"editor.fontFamily": "'MesloLGS NF', Hack, Consolas, 'Courier New', monospace",
	"explorer.compactFolders": false,
	"workbench.editor.labelFormat": "short",
	"editor.hover.delay": 600,
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"workbench.colorCustomizations": {
    "titleBar.activeForeground": "#555",
    "titleBar.activeBackground": "#67aff7",
    "titleBar.inactiveForeground": "#ccc",
    "titleBar.inactiveBackground": "#113457"
  }
}